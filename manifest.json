{
    "name": "very cool dark mode",
    "version": "4.0",
    "description": "google classroom, zoom, and synergy under the moonlight ðŸŒš",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "*://classroom.google.com/*"
            ],
            "js": [
                "index.js"
            ]
        },
        {
            "matches": [
                "*://ca-egusd-psv.edupoint.com/*"
            ],
            "js": [
                "edupoint.js"
            ]
        },
        {
            "matches": [
                "*://*.zoom.us/*"
            ],
            "js": [
                "zoom.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "confetti.js"
    ],
    "permissions": [
        "storage"
    ]
}